pipeline { 
    agent any
    stages {
        stage('Clone Git') {
            steps {
                git 'https://github.com/sambit417/SPE_MINIPROJECT.git'
            }
        }
        stage('Build Code') {
            steps {
                sh "chmod u+x cal.py"
                sh "./cal.py"
            }
        }
     stage('Test Code') {
            steps {
                sh "chmod u+x test.py"
                sh "./test.py"
            }
        }
    } 
}
