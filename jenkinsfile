pipeline { 
    agent any
    stages {
        stage('Clone Git') {
            steps {
                echo "hello"
            }
        }
        stage('Build Code') {
            steps {
                sh "chmod u+x cal.py"
                sh "./cal.py sqrt 25"
            }
        }
     stage('Test Code') {
            steps {
                sh "chmod u+x test.py"
                sh "./test.py"
            }
        }
    } 
}
